{
    "name": "palkesh/auto_crud",
    "description": "Laravel package for automatic CRUD generation with migrations, models, controllers, and blade views",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Palkesh",
            "email": "palkesh@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Palkesh\\AutoCrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Palkesh\\AutoCrud\\AutoCrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

